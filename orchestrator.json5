{
  devDependenciesLocation: "orchestrator-deps",
  processes: [
    {
      name: "httpServer",
      source: "3sig/3suite-http-server",
      sourceActions: [{ type: "chmod" }],
      config: {port: 4004, directory: "node_modules"},
      sourcePlatformConfig: {
        "win": {
          sourceActions: []
        }
      }
    },
    {
      name: "browser",
      source: "3sig/3suite-browser",
      sourceActions: [
        {
          type: "unzip"
        },
        //{
        //  type: "chmod",
        //  file: "3suite-browser.app/Contents/MacOS/3suite-browser"
        //}
      ],
      sourceExecOverride: "./3suite-browser.app/Contents/MacOS/3suite-browser",
      loadConfig: "browser",
      // Example: Platform-specific overrides for browser
      sourcePlatformConfig: {
        "linux-arm": {
          sourceExecOverride: "./3suite-browser-linux-arm64/3suite-browser"
        },
        "win": {
          sourceExecOverride: "./3suite-browser.exe"  // Windows executable
        }
      }
    },
    {
        name: "local-data-tool",
        sourceType: "local",
        localPath: "./",
        sourceFileType: "pattern-match",
        sourceFilePattern: "m*",
        sourceActions: [
          { type: "move", location: "./3db" }
        ],
        sourceExclude: true
    },
    // Example of using pattern matching instead of platform-based selection:
    // {
    //   name: "data-tool",
    //   source: "org/data-processor",
    //   sourceFileType: "pattern-match",
    //   sourceFilePattern: "data-*.zip",
    //   sourceActions: [{ type: "unzip" }, { type: "chmod" }],
    // },

    // Example of using move action to organize files:
    // {
    //   name: "organized-tool",
    //   source: "org/some-tool",
    //   sourceActions: [
    //     { type: "chmod" },
    //     { type: "move", location: "./bin", filename: "my-tool" }
    //   ],
    // },

    // Example of using local folder as source with platform-based selection:
    // {
    //   name: "local-tool",
    //   sourceType: "local",
    //   localPath: "/path/to/local/binaries",
    //   sourceActions: [{ type: "chmod" }],
    // },

    // Example of using local folder with pattern matching:
    // {
    //   name: "local-data-tool",
    //   sourceType: "local",
    //   localPath: "/path/to/local/files",
    //   sourceFileType: "pattern-match",
    //   sourceFilePattern: "tool-*.zip",
    //   sourceActions: [{ type: "unzip" }, { type: "chmod" }],
    // },

    // Example of a setup-only process that won't appear in final config.json5:
    // {
    //   name: "setup-utility",
    //   source: "org/setup-tool",
    //   sourceActions: [{ type: "chmod" }],
    //   sourceExclude: true,  // This process won't be included in config.json5
    // },
  ],

  configs: {
    browser: {
      url: "http://localhost:4004/3lib-config/main.js",
      fullscreen: false,
      disableCursor: false,

      windowWidth: 800,
      windowHeight: 600,
    }
  }
}
